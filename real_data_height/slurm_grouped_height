#!/bin/bash

#SBATCH --job-name=gVAMP_large_data

#SBATCH --time=2-00:00:00

#SBATCH --mem-per-cpu=5gb

#SBATCH --ntasks=30

#SBATCH --cpus-per-task=2

#SBATCH --output=/nfs/scistore13/mondegrp/kkoegler/gVAMP_grouped/real_data_height/results_grouped_height_217.log

module purge

ml gcc boost openmpi

module list

export OMP_NUM_THREADS=$SLURM_CPUS_PER_TASK

# printing out the details of the job

out_dir=/nfs/scistore13/mondegrp/kkoegler/gVAMP_grouped/real_data_height/output/
bed_file_loc=/nfs/scistore17/robingrp/human_data/adepope_preprocessing/PNAS_traits/HT/data/
phen_file_loc=/nfs/scistore17/robingrp/human_data/adepope_preprocessing/PNAS_traits/HT/data/
bed_name='ukb22828_UKB_EST_v3_ldp08_fd_HT_train'
phen_name='ukb22828_UKB_EST_v3_ldp08_fd_HT_train'
group_file_loc=/nfs/scistore17/robingrp/human_data/adepope_preprocessing/PNAS_traits/HT/data/ukb22828_UKB_EST_v3_ldp08_fd_HT_train.groupindex



echo "bed_name = " $bed_name

echo "phen_name = " $phen_name

# Specifying the total number of markers in the model (Mt)

Mt=2174071

vloc=/nfs/scistore13/mondegrp/kkoegler/gVAMP_grouped/real_data_height

time mpirun -np 30 $vloc/main_real_grouped_large_data.exe \
			        --bed-file $bed_file_loc/${bed_name}.bed \
                                --K 10 \
                                --group-file $group_file_loc \
                                --phen-files $phen_file_loc/${phen_name}.phen \
                                --N 414055  \
                                --Mt $Mt \
                                --out-dir $out_dir \
                                --out-name x1_hat_HT_${bed_name}_G1 \
                                --iterations 50 \
                                --num-mix-comp 23 \
                                --CG-max-iter 100 \
